echarset "utf-8";
/* CSS Document */

body{ margin:0; padding:0; font-size:12px; font-family:Verdana, Geneva, sans-serif;color:#555; line-height:20px;-webkit-text-size-adjust:none;}
a img{border:0;}
a:focus{outline:none}
h2{ font-size:14px; color:#003e7e; font-weight:bold; display:block; height:30px; line-height:30px;margin:0 0 10px 0;}
ul,li{ margin:0; padding:0; list-style:none;}
td{padding:5px 3px; overflow:hidden;}

.header{background-color:#666666; height:100px; margin-bottom:10px;
}
.DIV980{width:100%;margin:0 auto; clear:both; overflow:hidden; zoom:1; min-height:500px;}
a.logo{ display:inline-block;width:236px; float:left;height:100px; background:url(../images/active_network_logo.png) no-repeat center center;}


.header_nav{float:left;height:100px;width:744px;}

.nav_top{ height:20px; line-height:20px; margin:5px 0 15px 0;text-align:right;}
.nav_top a{color:#8DB6D8; text-decoration:none; font-size:10px; margin-right:10px;}
.nav_top a:hover{color:#ffc425;}
.nav_top a:hover .arrow{color:#ffc425; background-position:-20px 0;}

.nav{ text-align:right;padding-left:40px;}
.nav a{ width:80px; margin-left:5px; height:40px; float:left;display:inline-block; vertical-align:top;text-align:center; line-height:16px; color:#fff; font-weight:bold; text-decoration:none;} 
.nav a:hover{ border-bottom:3px #5191CD solid;}
a.lh40{ line-height:32px;}
img.arrow{ width:16px; height:16px; vertical-align:middle; background:url(../images/icons.png) no-repeat 0 0;}

.footer{float:bottom; height:110px; padding:20px 0; background-color:#e4e5e6; margin-top:10px; clear:both;}
a.logo_gray{display:inline-block;width:179px; float:left;height:110px; background:url(../images/logo_gray.png) no-repeat center center; border-right:1px solid #ccc;}
.footer_box{width:100%;margin:0 auto;}
.footer_nav{ float:left;border-left:1px solid #fff;width:359px;height:110px; padding-left:40px;}
.footer_nav a{ display:inline-block; float:left; width:40%; margin-right:5%; text-align:left; font-size:11px;text-decoration:none; line-height:26px; color:#666;}
.footer_nav a span{ font-family:"MS Serif", "New York", serif; font-size:10px; font-weight:bold; margin-right:5px;-webkit-text-size-adjust:none;}
.footer_nav a:hover{ color:#003e7e; }
.footer_nav a:hover span{ color:#ffc425;}

.footer_copyright{width:400px;height:100px; text-align:right; float:left; color:#666;padding-top:10px; font-size:11px;}
.footer_copyright p{ margin:0; line-height:30px;}
.footer_copyright p span{ font-weight:bold;}
.footer_copyright p a{ text-decoration:none; color:#666; font-size:11px;}
.footer_copyright p a:hover{ color:#003e7e; text-decoration:underline;}

.sidebar{width:190px; float:left;}
.sidebar a{ display:block;width:168px;height:36px; font-size:14px;line-height:36px; padding-left:10px; color:#333; border:#ccc 1px solid;border-bottom:none; text-decoration:none;}
.sidebar a:hover{ background-color:#EDEDED;}
.sidebar div:first-child{width:170px; height:38px;padding-left:10px;line-height:38px;background:url(../images/sidebar_top_bg.gif) no-repeat center bottom; text-align:left; font-size:14px; color:#003e7e; font-weight:bold;}
.sidebar div:last-child{width:180px; height:1500px; background:url(../images/sidebar_bottom_bg.gif) no-repeat center top;}
#sidebar_list a:first-child{ border-top:none;}
#sidebar_list span a:first-child{ border-top:#ccc 1px solid;}
#sidebar_list span a.thispage{ border-top:none;}
.sidebar span{ display:none;}
.sidebar span a{font-size:12px;padding-left:20px;width:158px;color:#666;}
a.a_open{ background:url(../images/arrow_open.gif) no-repeat right center;}
a.a_open:hover{background:#ededed url(../images/arrow_open.gif) no-repeat right center;}
a.a_close{background:url(../images/arrow_close.gif) no-repeat right center;}
a.a_close:hover{background:#ededed url(../images/arrow_close.gif) no-repeat right center;}
.sidebar a.thispage{color:#fff; cursor:default; border:none; background:url(../images/thisPage_bg.gif) no-repeat center left;width:180px; }

#btn_open_sidebar {display: none; width:14px; height:14px; text-align:center; float:left; cursor:pointer;margin-right:10px; margin-top:12px; background:url(../images/btn_sidebar_control.png) no-repeat 0 -14px;}
#btn_open_sidebar:hover{ background-position:-14px -14px;}
#btn_close_sidebar{ display: block; width:14px; height:14px; text-align:center; float:right; cursor:pointer;margin-right:10px; margin-top:12px; background:url(../images/btn_sidebar_control.png) no-repeat 0 0;}
#btn_close_sidebar:hover{ background-position:-14px 0;}



/*右侧主栏*/
.main_content{ overflow:hidden; zoom:1;padding-left:10px;}

.path{ height:40px; line-height:40px;border-bottom:1px solid #ccc; font-size:14px; margin-bottom:10px;}

/*日期*//*修改*/
.date{ width:181px; height:30px; line-height:28px; font-weight:bold; display:inline-block; float:right; margin-right:10px;text-align:center; color:#fff; font-size:16px; font-family:Verdana;background:url(../images/date_bg.png) no-repeat left top;}
.date a{ display:inline-block; vertical-align:top;width:15px; height:15px; margin-top:7px;background:url(../images/date_arrow.png) no-repeat;}
a.before{  background-position:-15px 0; margin-right:20px;}
a.before:hover{ background-position:0 0;}
a.next{ background-position:-15px -15px;margin-left:20px;}
a.next:hover{ background-position:0 -15px;}

/*主栏常规容器*/
.normal_box { clear:both; margin:10px 0;overflow:hidden; zoom:1;}

/*图文-常规文字列表*/
.normal_box .normal_list{}
.normal_box .normal_list li{ display:block;padding:5px 5px 5px 25px; list-style:none; background:url(../images/list_style_img.png) left top no-repeat; line-height:20px;}
.normal_box .normal_list li:nth-child(odd){ background-color:#f3f3f3;}
.normal_box .normal_list li:nth-child(even){ }
/*修改F0F9FF*/.normal_box .normal_list li:hover{ background-color:#E7F0F8; cursor:pointer; color:#333;}

.table_title{clear:both; margin:0 0% 0px 0;padding-left:0px;line-height:30px;font-size:12px; font-weight:bold; text-overflow:ellipsis;overflow:left; white-space:nowrap;}
.table_title:hover{word-break:keep-all; white-space:normal; overflow:visible;word-wrap:break-word;}


/*表头切换*/
.table_tab{border-bottom:3px solid #B7D3E8; margin:10px 1px 0 1px; line-height:24px;height:24px;}
.table_tab span { float:left;}
.table_tab span a{ float:left; font-weight:bold; margin-left:5px; display:block; vertical-align:middle; text-decoration:none; }
.table_tab span a.tab_on{ margin-right:10px; background-color:#B7D3E8;-webkit-border-radius:5px 5px 0 0;-moz-border-radius:5px 5px 0 0;padding:0 10px;line-height:24px;color:#333;}
.table_tab span a.tab_on:hover{background-color:#e3e3e3;}
.table_tab span a.tab_on:hover label{ background-color:#ffc425;}
.table_tab span a.tab_on h1{ display:inline-block;  }
.table_tab span a.tab_on label{margin-top:5px; background-color:#fff; float:left; }
.table_tab span a h1{display:none;margin:0; padding:0; font-weight:bold; font-size:12px; vertical-align:middle;}
.table_tab span a label{ margin-top:5px;display:inline-block; vertical-align:middle;width:14px; height:14px; background-color:#ffc425; color:#333; font-size:11px;-webkit-border-radius:7px;-moz-border-radius:7px; text-align:center; line-height:14px; font-weight:bold; margin-right:5px;}

/*全局表格特效*/
table{ background-color:#778; font-size:11px;table-layout:auto;}
table tr:nth-child(odd) td{ background-color:#e4e4e4;word-wrap:break-word}

td{ background-color:#f3f3f3; text-align:left; line-height:14px; text-overflow:ellipsis;overflow:hidden; white-space:nowrap;word-break:break-all;}
table td:hover{ cursor:pointer;text-shadow: 1px 1px 0 #ededed;}
table tr:hover td{ background-color:#B7D3E8; color:#555;  white-space:normal; overflow:visible;word-wrap:break-word;}
/*修改*/table tr.title td{ font-weight:bold; vertical-align:middle; background-color:#B7D3E8; color:#333; text-align:center;word-break:keep-all; white-space:normal; overflow:visible;word-wrap:break-word;}
/*修改table tr.title:hover td{  background-color:#ffc425; font-weight:normal;}*/
table tr:last-child td{height:15px;  repeat-x center top; cursor:default;}

td.green{ background-color:#9F6;}
td.yeelow{ background-color:#FF6;}
td.blue{ background-color:#0CF;}
table tr:nth-child(odd) td.green{ background-color:#9F6;}
table tr:nth-child(odd) td.yeelow{ background-color:#FF6;}
table tr:nth-child(odd) td.blue{ background-color:#0CF;}

/*打开全部*/
#open_all{width:980px;margin:0 auto; clear:both; height:10px;text-align:center; display:none; cursor:pointer;}

/*登录*/
#login { display:none;width:200px; position:absolute; left:50%; top:4px; margin-left:290px;filter:alpha(opacity=95); -moz-opacity:0.95; -khtml-opacity: 0.95; opacity: 0.95;}
#login .title{width:60px;color:#003e7e; text-decoration:none; font-size:10px;text-align:text;  border:1px solid #003e7e; border-bottom:none; float:right; background-color:#fff;}
#login .box{width:198px; border:1px solid #003e7e;float:right; background-color:#fff; border-top:none;}
#login p{ text-align:center; margin:10px 0; line-height:26px; clear:both;}
#login p label{ display:inline-block; width:60px;font-size:12px; text-align:right; color:#003e7e; margin-right:10px;}
#login p input{ display:inline-block; width:90px; background-color:#f3f3f3;line-height:26px;border:1px solid #003e7e; font-size:12px; color:#003e7e;padding:0 5px; vertical-align:middle;}
#login .btns input{ margin:0 10px;width:auto; cursor:pointer; line-height:20px; background-color:#003e7e; color:#fff;}
.select_class{
    border:1px solid #999;
    position:absolute;
    overflow:hidden;
    margin:-2px;
    padding:2px;
}

